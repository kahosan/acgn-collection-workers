{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "esnext"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "types": [
      "@cloudflare/workers-types"
    ],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  }
}
